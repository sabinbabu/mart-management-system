<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="558.0" prefWidth="873.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.billing.BillingFXMLDocumentController">
   <children>
      <ImageView fx:id="companyLogo" fitHeight="59.0" fitWidth="146.0" layoutX="368.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="companyAddress" layoutX="427.0" layoutY="80.0" text="Label" />
      <Separator layoutY="109.0" prefHeight="0.0" prefWidth="893.0" />
      <Label fx:id="welcomeUser" layoutX="718.0" layoutY="80.0" prefHeight="18.0" prefWidth="166.0" text="Label" />
      <Separator layoutY="149.0" prefHeight="3.0" prefWidth="894.0" />
      <TabPane layoutY="112.0" prefHeight="446.0" prefWidth="894.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="tabReport" onSelectionChanged="#tabReport" text="Report">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="textTextReport" layoutX="22.0" layoutY="14.0" prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab fx:id="tabInventory" text="Inventory">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
            </Tab>
            <Tab fx:id="tabSupplier" text="Supplier">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
            </Tab>
            <Tab fx:id="tabMyProfile" text="My Profile">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
            </Tab>
          <Tab fx:id="tabBilling" onSelectionChanged="#tabBilling" text="Billing">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="testTextBilling" layoutX="28.0" layoutY="22.0" prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab fx:id="tabEmployee" onSelectionChanged="#tabEmployee" text="Employee">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="398.0" prefWidth="861.0">
                     <children>
                        <Button fx:id="addEmployee" layoutX="786.0" layoutY="14.0" mnemonicParsing="false" onAction="#onAddEmployee" text="Add Employee" />
                        <TextField fx:id="empSearch" layoutX="14.0" layoutY="62.0" prefHeight="26.0" prefWidth="66.0" />
                        <Button fx:id="btnSearchEmp" layoutX="820.0" layoutY="103.0" mnemonicParsing="false" onAction="#onEmpSearchPressed" text="Search" />
                        <TextArea fx:id="empDisplay" layoutX="14.0" layoutY="140.0" prefHeight="259.0" prefWidth="862.0" />
                        <Button layoutX="14.0" layoutY="103.0" mnemonicParsing="false" onAction="#onDisplayAllEmployeePressed" text="Display All Employees" />
                        <ComboBox fx:id="employeeSearch" layoutX="115.0" layoutY="62.0" onAction="#onEmployeeSearchPressed" prefHeight="26.0" prefWidth="581.0" promptText="Select an employee" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
